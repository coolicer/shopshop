{
  "name": "shopshop",
  "version": "1.0.0",
  "description": "just an shop",
  "main": "app.js",
  "scripts": {
    "build-js": "browserify public/js/index.js | uglifyjs -mc > public/build/bundle.js",
    "build-css": "catw public/css/*.css > public/build/bundle.css",
    "build": "npm run build-js && npm run build-css",
    "watch-js": "watchify public/js/index.js -o public/build/bundle.js -dv",
    "watch-css": "catw public/css/*.css  -o public/build/bundle.css -v",
    "watch": "npm run watch-js & npm run watch-css",
    "start": "npm run build && node ./bin/www",
    "dev": "npm run watch & nodemon ./bin/www",
    "test": "mocha test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coolicer/shopshop.git"
  },
  "keywords": [
    "e-shop"
  ],
  "author": "coolicers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coolicer/shopshop/issues"
  },
  "homepage": "https://github.com/coolicer/shopshop",
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.12.2",
    "bootstrap": "^3.3.4",
    "connect-redis": "^2.2.0",
    "cookie-parser": "^1.3.4",
    "eventproxy": "^0.3.2",
    "express": "^4.12.3",
    "express-session": "^1.10.4",
    "lodash": "^3.5.0",
    "mailgun-js": "^0.6.8",
    "moment": "^2.9.0",
    "morgan": "^1.5.2",
    "mysql": "^2.5.5",
    "redis": "^0.12.1",
    "sequelize": "^2.0.4",
    "socket.io": "^1.3.5",
    "swig": "^1.4.2",
    "uuid": "^2.0.1",
    "validator": "^3.34.0"
  },
  "devDependencies": {
    "browserify": "^9.0.7",
    "catw": "^0.2.0",
    "mocha": "^2.2.1",
    "nodemon": "^1.3.7",
    "should": "^5.2.0",
    "supertest": "^0.15.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.1.0"
  }
}
